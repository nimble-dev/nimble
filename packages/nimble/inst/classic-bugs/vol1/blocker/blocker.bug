model {
   for (i in 1:Num) {
      rt[i] ~ dbin(pt[i], nt[i]);
      rc[i] ~ dbin(pc[i], nc[i]);
      logit(pc[i]) <- mu[i] 
      logit(pt[i]) <- mu[i] + delta[i];
      delta[i] ~ dnorm(d, tau);
      mu[i] ~ dnorm(0.0, 1.0E-5);
   }
   d ~ dnorm(0.0, 1.0E-6);
   tau ~ dgamma(1.0E-3, 1.0E-3);
   delta.new ~ dnorm(d,tau);
   sigma <- 1/sqrt(tau);
}
